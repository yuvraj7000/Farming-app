{
  "expo": {
    "name": "Kisan Bandhu",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/applogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.AgriNext.KisanBandhu",
      "googleServicesFile": "./assets/google-services.json",
      "networkSecurityConfig": "./network_security_config.xml"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/applogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/applogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          },
          "ios": {
            "flipper": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "75710f46-7a95-4ff9-835a-be83f49eb596"
      }
    }
  }
}